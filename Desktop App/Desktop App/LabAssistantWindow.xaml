<Window x:Class="Desktop_App.LabAssistantWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Desktop_App"
        mc:Ignorable="d"
        Title="Мед Лаб (Лаборант)" Height="450" Width="800" Background="#FF76E383" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="nazad" Background="White" Click="nazad_Click" Content="Назад" HorizontalAlignment="Left" Margin="14,22,0,0" VerticalAlignment="Top" Height="34" Width="73"/>
        <Label x:Name="lbUserLastname" HorizontalAlignment="Left" Margin="110,22,0,0" VerticalAlignment="Top" Height="34" Width="80"/>
        <Label x:Name="lbUserName" HorizontalAlignment="Left" Margin="195,22,0,0" VerticalAlignment="Top" Height="34" Width="67"/>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="33,86,183,30"/>
        <ComboBox x:Name="cm" HorizontalAlignment="Left" Margin="649,124,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Принятие биоматериала" Background="White" Click="Button_Click" HorizontalAlignment="Left" Margin="630,180,0,0" VerticalAlignment="Top" Height="37" Width="146"/>
    </Grid>
</Window>
